{
    "name": "owenvoke/torrent",
    "type": "library",
    "description": "A set of torrent management classes with support for Torrent v2.",
    "license": "MIT",
    "bin": [
        "bin/torrent"
    ],
    "require": {
        "php": "^7.3",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "humbug/box": "^3.2",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\Torrent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\Torrent\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
